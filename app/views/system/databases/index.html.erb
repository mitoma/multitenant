<h1>Listing system_databases</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Hostname</th>
    <th>Port</th>
    <th>Username</th>
    <th>Password</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @system_databases.each do |system_database| %>
  <tr>
    <td><%= system_database.name %></td>
    <td><%= system_database.hostname %></td>
    <td><%= system_database.port %></td>
    <td><%= system_database.username %></td>
    <td><%= system_database.password %></td>
    <td><%= link_to 'Show', system_database %></td>
    <td><%= link_to 'Edit', edit_system_database_path(system_database) %></td>
    <td><%= link_to 'Destroy', system_database, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Database', new_system_database_path %>
